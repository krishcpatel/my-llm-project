{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug My LLM App",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/server/main.go",
      "cwd": "${workspaceFolder}",
      "env": {
        "DB_HOST": "localhost",
        "DB_PORT": "5432",
        "DB_USER": "user",
        "DB_PASSWORD": "password",
        "DB_NAME": "mydb",
        "CHAT_MODEL": "deepseek-r1:14b",
        "EMBEDDING_MODEL": "snowflake-arctic-embed2",
      }
    }
  ]
}
